{
  "name": "mdns-mqtt-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "config": {
    "port": "8102"
  },
  "scripts": {
    "clean": "bunx rimraf ./dist",
    "watch": "vue-tsc --watch",
    "typecheck": "vue-tsc --noEmit",
    "dev": "vite --port=$npm_package_config_port --host",
    "build": "vite build",
    "preview": "vite preview",
    "android": "npx cap run android",
    "ios": "npx cap run ios",
    "sync": "npx cap sync",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "debug-android-s24": "bunx cap run android  --live-reload  --port $npm_package_config_port --target R3CX90A30EW",
    "debug-android-s10": "bunx cap run android  --live-reload --port $npm_package_config_port  --target RF8MC11LGDW",
    "debug-android-a15": "bunx cap run android  --live-reload --port $npm_package_config_port  --target RF8Y109W1JB",
    "debug-ios": "export PATH=\"/opt/homebrew/opt/ruby/bin:$PATH\" &&  bunx cap run ios --live-reload  --port $npm_package_config_port  --target 00008140-0002692C1E79801C",
    "ios-log": "idevicesyslog",
    "logcat:all": "adb logcat",
    "logcat:clear": "adb logcat -c",
    "logcat:xxx": "adb logcat com.haberlerm.mqttmdns:D *:S",
    "logcat:app": "adb logcat  Capacitor:D  ZeroConf:D  *:S",
    "logcat:app:device": "adb -d logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:emulator": "adb -e logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:mydevice": "adb -s R3CX90A30EW logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:output": "adb logcat -d -v threadtime Capacitor:D 'Capacitor/Console':D *:S > android_logs.txt",
    "run-on-galaxy-s24": "bunx cap run android --target R3CX90A30EW",
    "run-on-galaxy-s10": "bunx cap run android --target RF8MC11LGDW",
    "run-on-galaxy-a15": "bunx cap run android --target RF8Y109W1JB",
    "run-on-galaxy-pro-tab10": "bunx cap run android --target R52T40AW12D",
    "run-on-iphone": "bunx cap run ios --target 00008140-0002692C1E79801C",
    "run-on-ipad": "bunx cap run ios --target 7469941401580df80750b83afac3dec9bde0309a"
  },
  "dependencies": {
    "@capacitor/android": "^7.5.0",
    "@capacitor/cli": "^7.5.0",
    "@capacitor/core": "^7.5.0",
    "@capacitor/ios": "^7.5.0",
    "@capacitor/preferences": "^8.0.1",
    "@mhaberler/capacitor-zeroconf-nsd": "^5.0.3",
    "@tailwindcss/vite": "^4.1.18",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "mqtt": "^5.15.0",
    "process": "^0.11.10",
    "stream": "^0.0.3",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.4",
    "vue-tsc": "^3.2.4",
    "vite": "^7.3.1"
  }
}
